#!/usr/bin/env bash
# Find the PID of the process named "4-to_infinity_and_beyond"
PID=$(pgrep -f "4-to_infinity_and_beyond")

# Check if the PID is not empty
if [ -n "$PID" ]; then
    # If PID is found, kill the process
    kill "$PID"
    echo "Process 4-to_infinity_and_beyond (PID: $PID) has been stopped."
else
    # If PID is empty, the process is not running
    echo "Process 4-to_infinity_and_beyond is not running."
fi
